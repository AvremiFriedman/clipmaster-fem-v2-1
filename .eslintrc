{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "plugins": [
    "class-property"
  ],
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "global-require": 0,
    "react/jsx-filename-extension": 0
  }
}
